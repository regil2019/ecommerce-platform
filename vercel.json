{
  "buildCommand": "cd front-end && npm run build",
  "outputDirectory": "front-end/dist",
  "devCommand": "cd front-end && npm run dev",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://your-backend-url.vercel.app/api/$1"
    }
  ]
}